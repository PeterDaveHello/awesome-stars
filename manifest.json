{
  "background": {
    "scripts": [
      "utils.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Awesome stars"
  },
  "content_scripts": [
    {
      "css": [
        "awesome-stars.css"
      ],
      "js": [
        "jquery.min.js",
        "utils.js",
        "content.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "description": "Peek stars of repository on awesome list.",
  "manifest_version": 2,
  "name": "awesome-stars",
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "1.0.0"
}
